  # Comment the next line fi you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Libraries
  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON', '~> 4.0'
  pod 'AlamofireImage', '~> 3.3'
  pod 'SVProgressHUD'

end
